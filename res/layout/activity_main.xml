<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/Scale_library_mother_frame"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@drawable/bgs">
  	<LinearLayout 
	  	android:id="@+id/Scale_library_mother_string_linear"
	  	android:orientation="horizontal"
	  	android:layout_width="match_parent"
	  	android:layout_height="match_parent">
  		<View
	    	android:layout_width="5dp"
	    	android:layout_height="match_parent"
	    	android:background="#00000000"
	    />
	    <LinearLayout
	        android:id="@+id/Scale_library_6th_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView 
	            android:id="@+id/Scale_library_6th_string_img"
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <LinearLayout
	        android:id="@+id/Scale_library_5th_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView 
	       		android:id="@+id/Scale_library_5th_string_img"
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <LinearLayout
	        android:id="@+id/Scale_library_4th_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView 
	       		android:id="@+id/Scale_library_4th_string_img"
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <LinearLayout
	        android:id="@+id/Scale_library_3rd_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView
	        	android:id="@+id/Scale_library_3rd_string_img" 
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <LinearLayout
	        android:id="@+id/Scale_library_2nd_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView 
	        	android:id="@+id/Scale_library_2nd_string_img"
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <LinearLayout
	    	android:id="@+id/Scale_library_1st_string_linear"
	    	android:orientation="vertical"
	    	android:layout_width="0dp"
	    	android:layout_height="match_parent"
	    	android:layout_weight="1">
	        <ImageView 
	        	android:id="@+id/Scale_library_1st_string_img"
		    	android:cropToPadding="true"
		    	android:layout_width="wrap_content"
		    	android:layout_height="match_parent"
		    	android:layout_gravity="center"
		    	android:scaleType="fitXY"
	    	/>
	    </LinearLayout>
	    <View
	    	android:layout_width="5dp"
	    	android:layout_height="match_parent"
	    	android:background="#00000000"
	    />
    	</LinearLayout>
    	</FrameLayout>